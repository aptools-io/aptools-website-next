{
  "name": "aptools-web-frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "sitemap": "next-sitemap --config next-sitemap.config.js",
    "lint:style": "stylelint **/*.{scss,css}",
    "lint:style:fix": "stylelint **/*.scss --fix",
    "lint": "eslint \"./**/*.{ts,tsx,js,jsx}\" --quiet",
    "lint:fix": "eslint \"./**/*.{ts,tsx,js,jsx}\" --fix --color",
    "svg": "npx @svgr/cli -d ./src/components/svg ./src/assets/images/svg/ --typescript",
    "svg:combine": "svg-sprite --symbol --css --css-render-css --css-example --dest=out src/assets/images/svgCoins/*.svg"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.3",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "cookies-next": "^2.1.1",
    "echarts": "^5.4.2",
    "echarts-for-react": "^3.0.2",
    "formik": "^2.2.9",
    "lottie-react": "^2.4.0",
    "lottie-web": "^5.12.2",
    "moment": "^2.29.4",
    "next": "12.1.6",
    "next-sitemap": "^3.1.45",
    "next-translate": "^1.4.0",
    "nextjs-progressbar": "0.0.16",
    "prop-types": "^15.8.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-input-mask": "^2.0.4",
    "react-json-formatter": "^0.4.0",
    "react-lines-ellipsis": "^0.15.3",
    "react-paginate": "^8.1.3",
    "react-redux": "^8.0.2",
    "react-slick": "^0.29.0",
    "react-transition-group": "^4.4.2",
    "sass": "^1.52.1",
    "slick-carousel": "^1.8.1",
    "svg-sprite": "^2.0.2",
    "svgstore-cli": "^2.0.1",
    "swiper": "^8.3.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/node": "^17.0.39",
    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "8.0.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^12.4.2",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.5",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-scss": "^4.2.0",
    "typescript": "^4.7.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "prettier --write",
      "npm run lint:style:fix",
      "git add ."
    ],
    "**/*.{css,scss,sass,less}": [
      "npm run lint:style:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
